<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>

        <service id="ramsey.sculpin.bundle.codeblock_bundle.convert_listener" class="Ramsey\Sculpin\Bundle\CodeBlockBundle\ConvertListener">
            <tag name="kernel.event_subscriber" />
        </service>

        <service id="ramsey.twig.codeblock_extension" class="Ramsey\Twig\CodeBlock\CodeBlockExtension">
            <tag name="twig.extension" />
        </service>

    </services>

</container>
